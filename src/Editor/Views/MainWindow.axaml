<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
		xmlns:views="clr-namespace:Editor.Views"
		xmlns:editors="clr-namespace:Editor.Views.Editors"
        xmlns:vm="using:Editor.ViewModels"
        x:DataType="vm:MainWindowViewModel"
        x:Class="Editor.Views.MainWindow"
		WindowStartupLocation="CenterScreen"
        Title="Zeus Editor">

	<DockPanel>
		<views:TopLevelMenuView DockPanel.Dock="Top"/>
		<editors:WorldEditorView DockPanel.Dock="Bottom"/>
	</DockPanel>

</Window>